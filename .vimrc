"
" ~/.vimrc - Vim Configuration File
"  Name: ~/.vimrc 
"  Author: Blake Bartenbach
"
set nocp
set ws
set ic
set nu
set bg=dark
set hls
set cuc
set cul
set cc=80
set spl=en
set sps=best
set wh=50
set helpheight=50
set title
set smd
set ru
set noeb
set hlg=en
set ul=200
set sm
set ts=2
set sw=2
set sta
set sts=2
set et
set si
set cin
set ml
set ff=unix
set write
set wb
set bk
set bdir=$HOME/.vim/bak
set bex=.bak
set noaw
set fs
set dir=$HOME/.vim/swp
set swf
set uc=200
set ut=60000
set hi=10
set sh=/bin/bash
set ef=$HOME/.vim/error.log
set enc=utf-8
set lpl
syntax on
colo proxa256
