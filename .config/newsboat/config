# good ones: cyanism, light, nord is ok, schleichfahrt is red, simple isn't terrible, solarized is really good
include plain
auto-reload yes
reload-time 60
confirm-exit no
datetime-format "%A, %B %d %Y %r"
articlelist-title-format "%T - %U"
dirbrowser-title-format "%?0?Open Directory&amp;Save File? - %f"
filebrowser-title-format "%?0?Open Directory&amp;Save File? - %f"
dialogs-title-format "Dialogs"
itemview-title-format "%T"
external-url-viewer "extract_url"
show-keymap-hint yes
reload-threads 10
macro y set browser "mpv %u" ; open-in-browser ; set browser "elinks %u"
#notify-beep yes
highlight feedlist "---.*---" default default bold
highlight feedlist ".*0/0.." default default invis

browser "$(which qutebrowser) %u"
unbind-key ENTER
unbind-key j
unbind-key k
unbind-key J
unbind-key K
bind-key j down
bind-key k up
bind-key ENTER open
bind-key BACKSPACE quit
bind-key m bookmark # like mark
